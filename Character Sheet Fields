//ac calc
sheild, boolean
armour_bonus, int
max_dex, int
misc_bonus, int
ac, int
//AC
ac = 10
if (max_dex != 0) {
	if(sheild) {
    	ac +=2;
	}
  	if (dex > max_dex) {
    	ac += max_dex;
  	}
  	else {
    	ac += dex;
 	}
  	ac += armour_bonus;
}
